void <%UC_PREFIX%>_init() {
    
    // Init timer control registers to 0x00
    TCCR<%TIMER_NUMBER%>A = 0x00;
    TIMSK<%TIMER_NUMBER%> = 0x00;
    
    // Timer operates in normal mode (nothing to enable),
    // Prescale factor is <%UC_PRESCALE%> (CS<%TIMER_NUMBER%>0, CS<%TIMER_NUMBER%>1)
    TCCR<%TIMER_NUMBER%>A |= <%UC_PRESCALE_CODE%>;
    
    <%UC_OVERFLOWINTERRUPT_CODE%>
}